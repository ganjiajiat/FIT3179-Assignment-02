{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "House Pricing in Melbourne",
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ganjiajiat/FIT3179-Assignment-02/main/js/melbourne.topojson",
        "format": {"type": "topojson", "feature": "melbourne"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ganjiajiat/FIT3179-Assignment-02/main/data/Melbourne_housing.csv"
      },
      "transform": [{"filter": "datum.Longtitude > 0"}],
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longtitude", "type": "quantitative"},
        "latitude": {"field": "Lattitude", "type": "quantitative"},
        "size": {"value": 1},
        "color": {"field": "Price", "type": "quantitative"}
      }
    }
  ],
  "config": {}
}